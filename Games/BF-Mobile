   local Main = RenUi:Tab("Auto Farm","rbxassetid://6026568198")
   local Combat = RenUi:Tab("Combat","rbxassetid://7251993295")
   local Stats = RenUi:Tab("Stats","rbxassetid://7040410130")
   local Teleport = RenUi:Tab("Teleport","rbxassetid://6035190846")
   local Dungeon = RenUi:Tab("Dungeon","rbxassetid://7044284832")
