       BoardDesc.Parent = BoardFrame
       BoardDesc.BackgroundColor3 = _G.WindowBackgroundColor
       BoardDesc.BackgroundTransparency = 1.000
       BoardDesc.Position = UDim2.new(0, 25, 0, 3)
       BoardDesc.Size = UDim2.new(0, 300, 0, 30)
