           DropFrameList.BackgroundColor3 = _G.SectionColor
           DropFrameList.BackgroundTransparency = 1.000
           DropFrameList.Position = UDim2.new(0, 15, 0, 34)
           DropFrameList.Size = UDim2.new(0, 395, 0, 30)
           
