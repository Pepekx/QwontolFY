FirstPage:AddLine()

local Seegs = {}

for i,v in next, game:GetService("Workspace").Scripts.Eggs:GetChildren() do
   if v:IsA"Model" then
       table.insert(Seegs,v.Name)
   end
   end

local Eggs = Win:Tab("• Egg", "rbxassetid://8825651806")

local Egg = Eggs:NewSection("Bing Chiling")

Egg:AddDropdown("Select Eggs", Seegs, function(egglistfunc)
    _G.SelectEgg = egglistfunc
end)

Egg:AddToggle("Open Egg", false, function(value)
_G.Egg = value
while _G.Egg do task.wait()
local args = {[1] = tostring(_G.SelectEgg),[2] = "Single"}
game:GetService("ReplicatedStorage").Functions.Unbox:InvokeServer(unpack(args))
   end
end)

Egg:AddToggle("Open Egg Triple", false, function(value)
_G.Triple = value
while _G.Triple do task.wait()
local args = {[1] = tostring(_G.SelectEgg),[2] = "Triple"}
game:GetService("ReplicatedStorage").Functions.Unbox:InvokeServer(unpack(args))
   end
end)



