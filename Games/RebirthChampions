wait(5)

local args = {
    [1] = "Easter"
}

game:GetService("ReplicatedStorage").Events.Chest:FireServer(unpack(args))
wait(5)
end
end)


FirstPage:AddToggle("Auto Daily Spin" , false, function(value)
    _G.Spin = value
    while _G.Spin do
game:GetService("ReplicatedStorage").Functions.Spin:InvokeServer()
wait(5)
end
end)

